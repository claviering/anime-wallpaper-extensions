{
  "name": "Anime Wallpaper New Tab",
  "description": "Replace your new tab with a Anime Wallpaper",
  "version": "1.0",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "host_permissions": [
    "https://app.zichen.zone/api/*"
  ],
  "action": {
    "default_icon": {
      "16": "images/16x16.png",
      "32": "images/32x32.png",
      "48": "images/64x64.png",
      "128": "images/128x128.png"
    }
  },
  "icons": {
    "16": "images/16x16.png",
    "32": "images/32x32.png",
    "48": "images/64x64.png",
    "128": "images/128x128.png"
  }
}